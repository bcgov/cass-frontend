{
  "name": "cass",
  "version": "0.1.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "start": "node scripts/start.js",
    "start:dev": "API_URL=http://localhost:3001/ yarn start",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage",
    "prepare": "patch-package",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "update-api": "yarn add github:bcgov/cass-api",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "autoprefixer": "7.1.2",
    "babel-loader": "^7.1.4",
    "bootstrap": "^3.4.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "color": "^3.0.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "^1.1.11",
    "fs-extra": "3.0.1",
    "hal-rest-client": "^0.3.4",
    "html-webpack-plugin": "2.29.0",
    "interact.js": "^1.2.8",
    "interactjs": "^1.3.4",
    "istanbul": "^0.4.5",
    "cass-api": "github:bcgov/cass-api",
    "jest": "20.0.4",
    "map": "^1.0.1",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "open-iconic": "^1.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "rc-slider": "^8.6.1",
    "rc-tooltip": "^3.7.2",
    "react": "^16.4.2",
    "react-bootstrap": "^0.32.0",
    "react-calendar-timeline": "^0.22.0",
    "react-datetime": "^2.11.1",
    "react-dev-utils": "^4.2.1",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.4.2",
    "react-measure": "^2.0.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-svg-loader": "^2.1.0",
    "react-toastify": "^4.1.0",
    "react-toggle": "^4.0.2",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-modal": "^1.7.3",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "0.18.2",
    "superagent": "^3.8.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "webpack-version-file": "^0.1.3",
    "whatwg-fetch": "2.0.3",
    "yarn": "^1.22.0",
    "snyk": "^1.120.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.10",
    "@storybook/addon-knobs": "^3.3.10",
    "@storybook/addon-links": "^3.3.10",
    "@storybook/addon-options": "^3.3.10",
    "@storybook/addons": "^3.3.10",
    "@storybook/react": "^3.3.10",
    "@types/color": "^3.0.0",
    "@types/fetch-mock": "^5.12.2",
    "@types/http-proxy": "^1.16.2",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^21.1.8",
    "@types/moment": "^2.13.0",
    "@types/node": "^8.0.53",
    "@types/prop-types": "^15.5.2",
    "@types/rc-slider": "^8.2.3",
    "@types/rc-tooltip": "^3.4.11",
    "@types/react": "16.0.25",
    "@types/react-bootstrap": "^0.31.8",
    "@types/react-bootstrap-date-picker": "^4.0.4",
    "@types/react-dom": "^16.0.3",
    "@types/react-measure": "^2.0.2",
    "@types/react-redux": "^5.0.13",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-toggle": "^4.0.1",
    "@types/redux-form": "^7.2.5",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__addon-knobs": "^3.2.1",
    "@types/storybook__addon-links": "^3.0.2",
    "@types/storybook__react": "^3.0.6",
    "@types/superagent": "^3.5.8",
    "@yarnpkg/lockfile": "^1.0.2",
    "babel-core": "^6.26.0",
    "fetch-mock": "^6.0.0",
    "http-proxy": "^1.17.0",
    "isomorphic-fetch": "^2.2.1",
    "jest-to-match-shape-of": "^1.1.3",
    "patch-package": "^5.0.0",
    "postinstall-prepare": "^1.0.1",
    "redux-actions-assertions": "^1.3.0",
    "ts-jest": "^20.0.7",
    "ts-loader": "^2.3.7",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.2.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/app/infrastructure/*.{js,ts}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jestSetup.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "snyk": true
}
